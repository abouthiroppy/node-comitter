{
  "name": "node-comitter",
  "main": "bin/index.js",
  "author": "(http://about-hiroppy.com) abouthiroppy <git@about-hiroppy.com>",
  "version": "0.3.0",
  "license": "MIT",
  "description": "Commit formatter for node.",
  "files": [
    "bin"
  ],
  "keywords": [
    "node",
    "commit"
  ],
  "bin": {
    "node-comitter": "bin/index.js"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint": "eslint .",
    "test": "nyc ava",
    "test:watch": "nyc ava -w",
    "postcover": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true
  },
  "dependencies": {
    "axios": "^0.15.3",
    "chalk": "^1.1.3",
    "inquirer": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "conventional-changelog-cli": "^1.2.0",
    "eslint": "^3.15.0",
    "eslint-config-sky": "^1.4.3",
    "nyc": "^10.1.2",
    "pify": "^2.3.0"
  }
}
